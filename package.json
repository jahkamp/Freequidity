    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^2.39.0",
    "wagmi": "^2.19.4"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.8.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
